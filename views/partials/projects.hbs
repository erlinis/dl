 <div class='card'>
  <div class='card-containers'>
    <div class='card-header'>
      <h3>
        {{#if framework}}
          {{capitalize framework}}
        {{else}}
          {{capitalize platform}}
        {{/if}} Projects</h3>
    </div>
    <div class='card-body'>
        {{> projectInfoTitles framework=framework platform=platform}}
        {{> projectsInfo projects=projects}}
    </div>
  </div>
</div>
